{
    "version": 2,
    "builds": [
        {
            "src": "api/**/*.js",
            "use": "@vercel/node"
        },
        {
            "src": "public/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/airtable",
            "dest": "/api/airtable/index.js"
        },
        {
            "src": "/api/error",
            "dest": "/api/error.js"
        },
        {
            "src": "/css/(.*)",
            "dest": "/public/css/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/api/error.js"
        }
    ]
}